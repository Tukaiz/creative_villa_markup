.main-content{role: "main"}
  = render_partial 'subnav'
  .page-title
    .container
      %h1 4 Panel Kit
  .container
    = render_partial 'breadcrumbs'
    .row.product-details
      .col-md-6
        .preview-img.tab-content
          #preview1.tab-pane.fade.in.active
            %img{src: "/images/products/sample1.jpg"}/
            %a.zoom-preview{href: "#"}
              %i.fa.fa-search-plus>
              Zoom
          #preview2.tab-pane.fade
            %img{src: "/images/products/sample1.jpg"}/
            %a.zoom-preview{href: "#"}
              %i.fa.fa-search-plus>
              Zoom
          #preview3.tab-pane.fade
            %img{src: "/images/products/sample1.jpg"}/
            %a.zoom-preview{href: "#"}
              %i.fa.fa-search-plus>
              Zoom
        %ul#myTab.nav.nav-pills.preview-thumbs
          %li.active
            %a{"data-toggle" => "tab", href: "#preview1"}
              %img{src: "/images/products/sample1.jpg"}/
          %li
            %a{"data-toggle" => "tab", href: "#preview2"}
              %img{src: "/images/products/sample1.jpg"}/
          %li
            %a{"data-toggle" => "tab", href: "#preview3"}
              %img{src: "/images/products/sample1.jpg"}/
